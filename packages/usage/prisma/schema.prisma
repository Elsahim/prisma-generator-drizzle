generator drizzle {
  provider = "bunx prisma-generator-drizzle"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum UserType {
  TypeOne
  TypeTwo
}

model Team {
  id    Int    @id @default(autoincrement())
  name  String
  users User[]
}

model User {
  id       Int       @id @default(autoincrement())
  email    String    @unique
  name     String?
  bigint   BigInt?
  boolean  Boolean?
  datetime DateTime?
  decimal  Decimal?
  enum     UserType
  float    Float?
  json     Json?
  team     Team?     @relation(fields: [teamId], references: [id])
  teamId   Int?
}
